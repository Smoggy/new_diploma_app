
<table>
	<thead>
    <tr>
      <th>Full Name</th>
      <th>Email</th>
      <th>Address</th>
      <th>Phone</th>
      <th>Subjects</th>
    </tr>
  </thead>
   <tbody>
		<% @students.each do |student| %>
		<tr>
			<td><%= student.full_name %></td>
			<td><%= student.email %></td>
			<td><%= student.address %></td>
			<td><%= student.phone %></td>
			<td><%= student.subject_titles %></td>
		</tr>
		<% end %>
	</tbody>
</table>

<%= link_to "Add Student", new_student_path %>
